  <span class="k">if</span> <span class="p">(</span> <span class="o">!</span><span class="n">qword_1800171E8</span> <span class="p">)</span>
    <span class="k">return</span> <span class="p">(</span><span class="kt">unsigned</span> <span class="kt">int</span><span class="p">)</span><span class="n">dword_1800171E0</span><span class="p">;</span>
  <span class="o">*</span><span class="p">(</span><span class="n">_QWORD</span> <span class="o">*</span><span class="p">)</span><span class="n">qword_1800171E8</span> <span class="o">=</span> <span class="n">decrypt_strings_sub_180001000</span><span class="p">(</span><span class="n">v1</span><span class="p">,</span> <span class="mh">0xA1D45974</span><span class="p">);</span>
  <span class="o">*</span><span class="p">(</span><span class="n">_QWORD</span> <span class="o">*</span><span class="p">)(</span><span class="n">qword_1800171E8</span> <span class="o">+</span> <span class="mi">8</span><span class="p">)</span> <span class="o">=</span> <span class="n">decrypt_strings_sub_180001000</span><span class="p">(</span><span class="n">v1</span><span class="p">,</span> <span class="mh">0xAF11BC24</span><span class="p">);</span>
  <span class="o">*</span><span class="p">(</span><span class="n">_QWORD</span> <span class="o">*</span><span class="p">)(</span><span class="n">qword_1800171E8</span> <span class="o">+</span> <span class="mi">16</span><span class="p">)</span> <span class="o">=</span> <span class="n">decrypt_strings_sub_180001000</span><span class="p">(</span><span class="n">v1</span><span class="p">,</span> <span class="mh">0xB973B8DC</span><span class="p">);</span>
